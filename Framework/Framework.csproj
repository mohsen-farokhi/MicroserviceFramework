<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <PackageId>Farokhi.Microservice.Framework</PackageId>
    <Authors>Mohsen Farokhi</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Company>Mohsen.Farokhi</Company>
    <Product>Farokhi.Microservice.Framework</Product>
    <RepositoryUrl>https://github.com/mohsen-farokhi/MicroserviceFramework</RepositoryUrl>
    <PackageTags>Farokhi, Microservice, Framework</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="FluentResults" Version="2.5.0" />
    <PackageReference Include="MediatR" Version="9.0.0" />
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="5.0.10" />
  </ItemGroup>

</Project>
